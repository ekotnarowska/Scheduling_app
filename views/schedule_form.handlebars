<form action="/schedules" method="POST" class="mb-4">
<div class="form-group mb-2">
    <select class="form-select" aria-label="Default select example">
        <option selected>Choose Participant</option>
        <option value="1">One</option>
        <option value="2">Two</option>
        <option value="3">Three</option>
</select>
</div>
<div class="form-group mb-2">
     <select class="form-select" aria-label="Default select example">
        <option selected>Choose Day</option>
        <option value="1">One</option>
        <option value="2">Two</option>
        <option value="3">Three</option>
</select>
</div>
<div class="form-group mb-2">
        <select class="form-select" aria-label="Default select example">
        <option selected>Start at</option>
        <option value="1">One</option>
        <option value="2">Two</option>
        <option value="3">Three</option>
</select>
</div>
<div class="form-group mb-2">
    <select class="form-select" aria-label="Default select example">
        <option selected>End at</option>
        <option value="1">One</option>
        <option value="2">Two</option>
        <option value="3">Three</option>
</select>
</div>
<a href='/schedules' role="button"  class="btn btn-light btn-block me-2 mt-4">Cancel</a>
<input type="submit" value="Add schedule" class="btn btn-secondary btn-block mt-4">
</form>